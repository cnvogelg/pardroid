CONFIG_MAKESYS=cmake

CONFIG_ARCH=rp2
CONFIG_MCU=rp2040
CONFIG_MACH=pico
CONFIG_PTR_BITS=32

CONFIG_BAUD_RATE=115200
CONFIG_STACK_SIZE=4096

CONFIG_DRIVER_SDCARD=y
CONFIG_DRIVER_SDCARD_SPI_CS=0
CONFIG_DRIVER_SDCARD_CRC=n

CONFIG_DRIVER_ENC28J60=y
CONFIG_DRIVER_ENC28J60_SPI_CS=1

CONFIG_DRIVER_WIZNET=y
CONFIG_DRIVER_WIZNET_SPI_CS=1
CONFIG_DRIVER_WIZNET_NUM_SOCKETS=4
